@model Visit
@{
    Layout = "~/Views/Crm/_LayoutContact.cshtml";
    ViewData.SetTitleAndHeading(CrmController.ViewNames.Health.Visit.ToCrmHeading());
}
<dl class="dl-horizontal">
    @Html.DescriptionListElement(model => model.VisitId)
    @Html.DescriptionListElement(model => model.VisitType)
    @Html.DescriptionListElement(model => model.VisitStartDd)
    @Html.DescriptionListElement(model => model.VisitEndDd)
    @Html.DescriptionListElement(model => model.ForeignId)
    @Html.DescriptionListElement(model => model.AdmissionType)
    @Html.DescriptionListElement(model => model.InpatientDays)
    @Html.DescriptionListElement(model => model.AdmissionFromEmergencyRoom)
    @Html.DescriptionListElement(model => model.VisitMedicalClaimLines)
    @foreach (var line in Model.VisitMedicalClaimLines)
    {
        @Html.DescriptionListElement(model => line)
    }
</dl>
