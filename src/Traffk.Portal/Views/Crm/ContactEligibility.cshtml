@model Traffk.Bal.Data.Rdb.TraffkTenantModel.Eligibility
@{
    Layout = "~/Views/Crm/_LayoutContact.cshtml";
    ViewData.SetTitleAndHeading(CrmController.ViewNames.Health.Eligibility.ToCrmHeading());
}
<dl class="dl-horizontal">
    @Html.DescriptionListElement(model => model.MemberId)
    @Html.DescriptionListElement(model => model.EligibilityId)
    @Html.DescriptionListElement(model => model.Contact.AsPerson.FirstName)
    @Html.DescriptionListElement(model => model.Contact.AsPerson.MiddleName)
    @Html.DescriptionListElement(model => model.Contact.AsPerson.LastName)
    @Html.DescriptionListElement(model => model.CobraL.LookupValue)
    @Html.DescriptionListElement(model => model.Contact.AsPerson.Gender)
    @Html.DescriptionListElement(model => model.Contact.AsPerson.DateOfBirth)
    @Html.DescriptionListElement(model => model.MemberAddress.AddressLine1)
    @Html.DescriptionListElement(model => model.MemberAddress.AddressLine2)
    @Html.DescriptionListElement(model => model.MemberAddress.City)
    @Html.DescriptionListElement(model => model.MemberAddress.State)
    @Html.DescriptionListElement(model => model.MemberAddress.PostalCode)
    @foreach (var phoneNumber in Model.Contact.ContactDetails.PhoneNumbers)
    {
        <p>@String.Format("{0:(###) ###-####}", phoneNumber)</p>
    }

    @Html.DescriptionListElement(model => model.MemberRelationshipL.LookupValue)
    @Html.DescriptionListElement(model => model.CoverageTypeL.LookupValue)
    @Html.DescriptionListElement(model => model.MedicalEffDd.CalendarDate)
    @Html.DescriptionListElement(model => model.MedicalTermDd.CalendarDate)
    @Html.DescriptionListElement(model => model.PrescriptionEffDd.CalendarDate)
    @Html.DescriptionListElement(model => model.PrescriptionTermDd.CalendarDate)
    @Html.DescriptionListElement(model => model.DentalEffDd.CalendarDate)
    @Html.DescriptionListElement(model => model.DentalTermDd.CalendarDate)
    @Html.DescriptionListElement(model => model.VisionEffDd.CalendarDate)
    @Html.DescriptionListElement(model => model.VisionTermDd.CalendarDate)
    @Html.DescriptionListElement(model => model.LongTermDisabilityEffDd.CalendarDate)
    @Html.DescriptionListElement(model => model.LongTermDisabilityTermDd.CalendarDate)
    @Html.DescriptionListElement(model => model.ShortTermDisabilityEffDd.CalendarDate)
    @Html.DescriptionListElement(model => model.ShortTermDisabilityTermDd.CalendarDate)

</dl>
