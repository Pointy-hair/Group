@using static TraffkPortal.Controllers.TenantsController
@model Traffk.Bal.Data.Rdb.Tenant
@{
    ViewData.SetTitleAndHeading("Delete Tenant", Model.TenantName);
}
<h3>Are you sure you want to delete this?</h3>
<div>
    @await Html.PartialAsync("_DetailsPartial", Model)
    <form asp-action="Delete">
        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-default btn-primary btn-large" /> 
            <button type="button" class="btn btn-default" asp-action="Index">Back to List</button>
        </div>
    </form>
</div>
